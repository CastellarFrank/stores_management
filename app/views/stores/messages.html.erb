<div class="page-header">
    <div class="icon">
        <span class="ico-arrow-right"></span>
    </div>
    <h1><%= "All messages for store: [#{@store.name}]"%><small>System</small></h1>
</div>

<div class="row-fluid">
    <div class="span12">
        <div class="block">
            <%if @contacts.any?%>
                <%= render partial: 'contacts/contact', collection: @contacts%>
            <%else%>
                <i><%= "This store doesn't have any message." %></i>
            <%end%>
        </div>
    </div>
</div>